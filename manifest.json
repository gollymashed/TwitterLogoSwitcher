{
  "manifest_version": 3,
  "name": "de-x",
  "description": "This extension replaces the new Twitter logo with the correct one.",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "storage", "scripting", "favicon"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_icon": {
      "128": "images/icon-128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "images/icon-128.png"
  }
}
